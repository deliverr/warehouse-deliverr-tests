{
  "name": "warehouse-deliverr-tests",
  "version": "1.0.0",
  "description": "A test suite for 3PLs to run and test their Deliverr integration",
  "main": "index.js",
  "scripts": {
    "test1": "jest tests/TestPart1.spec.ts --forceExit",
    "test2": "jest tests/TestPart2.spec.ts --forceExit",
    "test3": "jest tests/TestPart3.spec.ts --forceExit",
    "test4": "jest tests/TestPart4.spec.ts --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deliverr/warehouse-deliverr-tests.git"
  },
  "author": "Vicki Chow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deliverr/warehouse-deliverr-tests/issues"
  },
  "homepage": "https://github.com/deliverr/warehouse-deliverr-tests#readme",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "axios": "^0.18.0",
    "dotenv": "^8.2.0",
    "jest": "^23.6.0",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.16",
    "ts-jest": "^24.2.0"
  },
  "jest": {
    "automock": false,
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec|uspec|ispec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
